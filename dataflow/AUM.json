{
	"name": "AUM",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "InvestorBalances",
						"type": "DatasetReference"
					},
					"name": "InvestorBalances"
				},
				{
					"dataset": {
						"referenceName": "DAAccountBalances",
						"type": "DatasetReference"
					},
					"name": "DAAccountBalances"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as long,",
				"          InvestorId as long,",
				"          FundId as integer,",
				"          CurrentNN as decimal(22,6),",
				"          CurrentSP as decimal(22,6)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> InvestorBalances",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> DAAccountBalances",
				"InvestorBalances select(mapColumn(",
				"          CurrentNN,",
				"          CurrentSP",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"DAAccountBalances select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: false) ~> select2"
			]
		}
	}
}
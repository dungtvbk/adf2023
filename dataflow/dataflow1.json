{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dcx",
						"type": "DatasetReference"
					},
					"name": "areas"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Id as integer,",
				"          OrganizationId as integer,",
				"          Name as string,",
				"          CreationTime as timestamp,",
				"          LastModificationTime as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     limit: 1000,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> areas"
			]
		}
	}
}